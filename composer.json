{
    "name": "xiaoyao-work/Lee",
    "description": "The PHP Framework.",
    "keywords": ["PHP framework", "Lee"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.4.0",
        "lee/blade": "1.*",
        "illuminate/database": "5.4.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Lee\\": "lee/src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }, {
            "type": "vcs",
            "url": "git@github.com:xiaoyao-work/lee-blade.git"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }

}
